*****************************************
* Glavni meni za prihode PDV robe
*****************************************
LOCAL Izbor, nPredhIzbor

nPredhIzbor = 1
DO WHILE .T.

	MainWnd("KNJIGA PRIHODA KAFA,PI'E,CIGARTE")

	Izbor = nPredhIzbor
	@ 8,27  PROMPT " UNOS KAFA,PI'E,CIGARETE   " MESSAGE "UNOS PRIHODA ZA PRDATU KAFU,PI'E,CIGARETE"
	@ 10,27 PROMPT " IZMENA  PRIHODA KA.PI.CIG " MESSAGE "IZMENA STAVKE PRIHODA KAFA,PI'E,CIGARETE"
	@ 12,27 PROMPT " LISTANJE PRIHODA K.P.C.   " MESSAGE "LISTANJE KNJIGE OD POCETKA DO KRAJA"
	@ 14,27 PROMPT " UPITI  ZBIROVA KA.PI.CIG. " MESSAGE "ZBIROVI PO PERIODIMA KAFA,PI'E,CIGARETE"
	@ 16,27 PROMPT " STAMPANJE KNJIGE PRIHODA  " MESSAGE "STAMPANJE PO PERIODU PRIHODA OD KAFE,PI'A I CIGARETA"

	CLEAR TYPEAHEAD
	MENU TO Izbor
	* nPredhIzbor = Izbor

	DO CASE
		CASE Lastkey() = 27
			EXIT
		CASE Izbor = 1
			DO KPCPAZ
		CASE Izbor = 2
			DO KPCIZM
		CASE Izbor = 3
			DO KPCLIST
		CASE Izbor = 4
			DO KPCZBIR
		CASE Izbor = 5
			DO KPCSTAMP
	ENDCASE

ENDDO

RETURN
