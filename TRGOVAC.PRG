*****************************************
* Glavni meni za izbor posla KEPO knjige
*****************************************
LOCAL Izbor, nPredhIzbor

nPredhIzbor = 1
DO WHILE .T.

	MainWnd("KEPU KNJIGA")

	Izbor = nPredhIzbor
	@ 07,27 PROMPT " UNOS U KEPU KNJIGU      " MESSAGE "UNOS U KEPU KNJIGU"
	@ 09,27 PROMPT " IZMENA U KEPU KNJIZI    " MESSAGE "IZMENA STAVKE U KEPU KNJIZI"
	@ 11,27 PROMPT " BRISANJE U KEPU KNJIZI  " MESSAGE "BRISANJE STAVKE IZ KEPU KNJIGE"
	@ 13,27 PROMPT " LISTANJE KEPU KNJIGE    " MESSAGE "LISTANJE KNJIGE PO OGRANIZACIONIM JEDINICAMA"
	@ 15,27 PROMPT " ZBIROVI KEPU KNJIGE     " MESSAGE "ZBIROVI OD DATUMA DO DATUMA"
	@ 17,27 PROMPT " PUNJENJE IZ KALKULACIJA " MESSAGE "PUNI TRG IZ KALKULACIJA"
	@ 19,27 PROMPT " PUNJENJE IZ PK1 KNJIGE  " MESSAGE "PUNI TRG IZ RASHODA"
	@ 21,27 PROMPT " STAMPANJE KEPU KNJIGE   " MESSAGE "STAMPANJE KEPU KNJIGE OD DATUMA DO DATUMA"

	CLEAR TYPEAHEAD
	MENU TO Izbor
	* nPredhIzbor = Izbor

	DO CASE
		CASE Lastkey() = 27
			EXIT
		CASE Izbor = 1
			DO TRGUN
		CASE Izbor = 2
			DO TRGIZ
		CASE Izbor = 3
			DO TRGBRIS
		CASE Izbor = 4
			DO TRGLIS
		CASE Izbor = 5
			DO TRGZB
		CASE Izbor = 6
			DO PUNKTRG
		CASE Izbor = 7
			DO PREPTRG
		CASE Izbor = 8
			DO TRGSTAM
			EXIT
	ENDCASE

ENDDO

RETURN
