*******************************************
* Glavni meni za Komision
*******************************************
LOCAL Izbor

DO WHILE .T.

	Naslov()
	Esckey()
	
	@ 2,21 SAY "GLAVNI MENI KOMISIONA ZA IZBOR POSLA"
	Izbor = 1
	@ 8,27  PROMPT " MATICNI PODACI FIRME    " MESSAGE "OSNOVNI PODACI O FIRMI CIJE SE KNJIGOVODSTVO VODI"
	@ 10,27 PROMPT " KALKULACIJE             " MESSAGE "MODUL ZA OBRADU KALKULACIJA"
	@ 12,27 PROMPT " IZVESTAJI               " MESSAGE "IZVESTAJI O POREZIMA I PRIHODIMA"
	@ 14,27 PROMPT " POVRACAJ ROBE           " MESSAGE "POVRACAJ ROBE KOMINTENTU"
	@ 16,27 PROMPT " NIVELACIJE              " MESSAGE "NIVELACIJE KOMISION"
	@ 19,27 PROMPT " SREDI KALKULACIJE 50%   " MESSAGE "SREDJUJE KALKULACIJE SA 50 % ZA KOMISIONARA"
	@ 20,27 PROMPT " PUNI PK 1 POSLE KAL.50% " MESSAGE "NAPUNI PK1 POSLE SREDJIVANJA KALKULACIJA SA 50 %"
	@ 21,27 PROMPT " BRISE POVRACAJE OD - DO " MESSAGE "BRISE POVRACAJE OD BROJA DO BROJA"

	CLEAR TYPEAHEAD
	MENU TO Izbor 

	DO CASE
		CASE Lastkey() = 27
		EXIT
		CASE IZBOR = 1
			DO MATGLMEN
		CASE IZBOR = 2
			* DO KALGLKOM
         MessageBox("KALGLKOM")
		CASE IZBOR = 3
			* DO IZVEMEN
         MessageBox("IZVEMEN")
		CASE IZBOR = 4
			* DO KPOVROB 
         MessageBox("KPOVROB")
		CASE IZBOR = 5
			* DO NKOMMEN
         MessageBox("NKOMMEN")
		CASE IZBOR = 6
			* DO SREDK05
         MessageBox("SREDK05")
		CASE IZBOR = 7
			* DO SREDR05
         MessageBox("SREDR05")
		CASE IZBOR = 8
			* DO KOMBPOVS
         MessageBox("KOMBPOVS")
	ENDCASE
ENDDO

CLEAR SCREEN
RETURN
