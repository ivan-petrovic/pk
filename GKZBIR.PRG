* - nije pregledano - *
CLEAR SCREEN
SET DATE GERMAN
*USE GKPRI INDEX GKPINDK
USE RASHODN INDEX RASHINDN
SET FILTER TO ((RPV + RPN + RPN1 + RPBEZ + RPBEZ06 + RPBEZ2 + RPBEZ4) > 0 .AND. RSK = 0)
GO TOP
DO WHILE .T.
   GO TOP
   ESCKEY()
   DAT1 = DATE()
   DAT2 = DATE()
   WUKV = VAL("000000000000.00")
   WUKN = VAL("000000000000.00")
   WUKN1 = VAL("000000000000.00")
   WUKB = VAL("000000000000.00")
   WUKB06 = VAL("000000000000.00")
   WUKB2 = VAL("000000000000.00")
   WUKB4 = VAL("000000000000.00")
   @ 2,10 SAY "UKUPNI PRIHOD OD DATUMA" GET DAT1
   @ 2,43 SAY "DO DATUMA" GET DAT2
   READ
   *
   IF Lastkey() = 27
      CLEAR SCREEN
      SET FILTER TO
      USE
      RETURN
   ENDIF
   DO WHILE .T.
      IF Eof()
         EXIT
      ENDIF
      IF DTOS(RDK) >= DTOS(DAT1) .AND. DTOS(RDK) <= DTOS(DAT2)
         WUKV = ROUND(WUKV + RPV,2)
         WUKN = ROUND(WUKN + RPN,2)
         WUKN1 = ROUND(WUKN1 + RPN1,2)
         WUKB = ROUND(WUKB + RPBEZ,2)
         WUKB06 = ROUND(WUKB06 + RPBEZ06,2)
         WUKB2 = ROUND(WUKB2 + RPBEZ2,2)
         WUKB4 = ROUND(WUKB4 + RPBEZ4,2)
      ENDIF
      SKIP
   ENDDO
   @ 4,20 SAY "UKUPNI PRIHOD ZA DATI PERIOD JE"
   @ 6,10 SAY "PRIHOD PO VISOJ  "
   @ 8,10 SAY "PRIHOD PO NIZOJ  "
   @ 10,10 SAY "PRIHOD PO NIZ. 12"
   @ 12,10 SAY "PRIHOD OSL. P/P  "
   @ 14,10 SAY "PRIHOD PO O.6    "
   @ 16,10 SAY "PRIHOD PO 2      "
   @ 18,10 SAY "PRIHOD PO 4      "
   @ 20,10 SAY "UKUPAN PRIHOD    "
   *SET COLO TO /W
   @ 06,30 SAY WUKV PICTURE "999,999,999,999.99"
   @ 08,30 SAY WUKN PICTURE "999,999,999,999.99"
   @ 10,30 SAY WUKN1 PICTURE "999,999,999,999.99"
   @ 12,30 SAY WUKB PICTURE "999,999,999,999.99"
   @ 14,30 SAY WUKB06 PICTURE "999,999,999,999.99"
   @ 16,30 SAY WUKB2 PICTURE "999,999,999,999.99"
   @ 18,30 SAY WUKB4 PICTURE "999,999,999,999.99"
   @ 20,30 SAY ROUND(WUKV + WUKN + WUKN1 + WUKB + WUKB06 + WUKB2 + WUKB4,2) PICTURE "999,999,999,999.99"
   *SET COLO TO
   WAIT " "
   CLEAR SCREEN
   SET FILTER TO
   USE
   RETURN
ENDDO
        
