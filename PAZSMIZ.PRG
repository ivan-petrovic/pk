SET CONFIRM ON
SET DATE GERMAN
CLEAR SCREEN
ESCKEY()
@ 1,0 TO 24,79 DOUBLE
D = 1
DO WHILE D = 1
@ 1,15 SAY " IZMENA PODATAKA U KNJIZI PRIHODA I RASHODA "
*
@ 2,5 SAY "REDNI BROJ                   "
@ 2,35 SAY WRRB PICTURE "9999"
@ 2,40 SAY "DATUM KNJIZENJA"
@ 2,56 SAY WRDK
@ 3,5 SAY "BROJ DOKUMENTA               " &&GET WRBR
@ 4,5 SAY "OPIS KNJIZENJA               "
@ 4,35 SAY ROPIS
@ 5,5 SAY "DATUM DOKUMENTA ZA KNJIZNENJE" && GET WRDATDOK
@ 6,5 SAY "PRIHOD OD ROBE OPSTA         "  GET WRPV PICTURE "999999999999.99"
@ 7,5 SAY "PRIHOD OD ROBE NIZA          "  GET WRPN PICTURE "999999999999.99"
@ 7,52 GET WRPN1 PICTURE "999999999999.99"
@ 8,5 SAY "PRIHOD OD ROBE BEZ POREZA    " GET WRPBEZ PICTURE "999999999999.99"
@ 8,52 GET wrpbez06 PICTURE "999999999999.99"
@ 9,5 SAY "PRIHOD OD USLUGA SA P/P      " GET WRUSP PICTURE "999999999999.99"
@ 9,52 GET wrpbez2 PICTURE "999999999999.99"
@ 10,5 SAY "PRIHOD OD USLUGA BEZ P/P     " GET WRUSBEZP PICTURE "999999999999.99"
@ 10,52 GET wrpbez4 PICTURE "999999999999.99"
@ 11,5 SAY "PLACENI POREZ PROIZ.USLU.   " && GET WRPLACP PICTURE "999999999999.99"
@ 12,5 SAY "SVEGA PRIHODA                " GET WRSVEP PICTURE "999999999999.99"
@ 13,5 SAY "NAB.VRED.ROBE,REPRO.REZ.DEL " && GET WRNVRRR PICTURE "999999999999.99"
@ 14,5 SAY "VRED.SOPSTVEN.PROIZVODA     " && GET WRVSP PICTURE "999999999999.99"
@ 15,5 SAY "RAZLIKA U CENI,PROVIZIJA    " && GET WRRUCP PICTURE "999999999999.99"
@ 16,5 SAY "OBRAC.POR.AKCIZA,P/P USLUGA " && GET WROBPOR PICTURE "999999999999.99"
@ 17,5 SAY "PROD.VRADNOST ROB. I PROIZ. " && GET WRPRODV PICTURE "999999999999.99"
@ 18,5 SAY "MAT. I NEMAT. TROS. I AMORT." && GET WRMIN PICTURE "999999999999.99"
@ 19,5 SAY "DOP. I BRUTO ZARADE ZAPOSLE." && GET WRDOP PICTURE "999999999999.99"
@ 20,5 SAY "OSTALI RAS. I RAS. FINANSIR." && GET WRRASF PICTURE "999999999999.99"
@ 21,5 SAY "RASHODI                     " && GET WRRASUK PICTURE "999999999999.99"
@ 22,5 SAY "ZIRORACUN UPLACENO/ISPLACENO" && GET WRZIRU PICTURE "999999999999.99"
* @ 22,52 GET WRZIRI PICTURE "999999999999.99"
*
READ
*
 IF Lastkey() = 27
 V = 1
 RETURN
 ENDIF
D = 0
*
ENDDO D
*
RETURN
