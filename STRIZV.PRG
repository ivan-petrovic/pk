*******************************************
* Glavni meni za Izvestaje
*******************************************
LOCAL Izbor, nPredhIzbor

nPredhIzbor = 1
DO WHILE .T.

	MainWnd("PRODATI ARTIKLI I POREZ STR-a")

	Izbor = nPredhIzbor
	@ 6 ,27 PROMPT " UPIT U DNEVNI PAZAR STR    " MESSAGE "SPISAK DNEVNOG PAZARA PO ARTIKLIMA STR-a"
	@ 8 ,27 PROMPT " STAMPA DNEVNOG PAZARA STR  " MESSAGE "STAMPA DNEVNOG PAZARA PO ARTIKLIMA STR-a"
	@ 10,27 PROMPT " UPIT U DNEVNI PAZAR KOMISI " MESSAGE "SPISAK DNEVNOG PAZARA PO ARTIKLIMA KOMISIONA"
	@ 12,27 PROMPT " STAMPA DNEVNOG PAZARA KOMI " MESSAGE "STAMPA DNEVNOG PAZARA PO ARTIKLIMA KOMISIONA"
	@ 14,27 PROMPT " POREZ ZA PLACANJE STR(DAN) " MESSAGE "DNEVNI OBRACUN POREZA STR-a"
	@ 16,27 PROMPT " POREZ ZA PLACANJE STR(PER) " MESSAGE "PERIODICNI OBRACUN POREZA STR-a"
	@ 18,27 PROMPT " BRISANJE STAVKE DNEV.PAZAR " MESSAGE "BRISE STAVKU PAZARA (STR, KOMISION), VRACA KOL. NA LAGER"

	CLEAR TYPEAHEAD
	MENU TO Izbor
	* nPredhIzbor = Izbor

	DO CASE
		CASE Lastkey() = 27
			EXIT
		CASE Izbor = 1
			DO PAZUP
		CASE Izbor = 2
			DO PAZPRAV
		CASE Izbor = 3
			DO KPAZUP
		CASE Izbor = 4
			DO KPAZPRAV
		CASE Izbor = 5
			DO PORSTRD
		CASE Izbor = 6
			DO PORSTR30
		CASE Izbor = 7
			DO PAZBSK
	ENDCASE

ENDDO

RETURN
