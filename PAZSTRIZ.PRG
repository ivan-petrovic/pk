* - nije pregledano - *
SET CONFIRM ON
SET DATE GERMAN
SET MESSAGE TO 24 CENTER
PUBLIC DATUM,V
DATUM = DATE()
WDAT = DATE()
USE RASHODN INDEX RASHINDN,RASHINKN
SET FILTER TO RSK = 0
GO TOP
B = LASTREC()
B = B + 1
DO WHILE .T.
   CLEAR SCREEN
   WDAT = DATE()
   WBROJ = Space(10)
   wzbir = val("000000000000.00")
   @ 10,10 SAY "UNESITE DATUM PAZARA" GET WDAT
   READ
   IF Lastkey() = 27
      EXIT
   ENDIF
   SEEK WDAT
   IF Found()
          DO WHILE WDAT = RDK
             wzbir = round(rpbez06+rpbez2+rpbez4,2)
            IF (wzbir > 0 .or. RPV > 0 .OR. RPN > 0 .OR. RPN1 > 0 .OR. RPBEZ > 0 .OR. RUSP > 0 .OR. RUSBEZP > 0) .AND. RSK = 0
              * INDDAT = 1
               EXIT
            ENDIF
               wzbir = 0
               SKIP
          ENDDO
   ELSE
      @ 20,10 SAY "NEMA PAZARA ZA TAJ DATUM "
      WAIT " "
      @ 20,10 SAY REPLI(" ",70)
      LOOP
   ENDIF
   V = 0
   DN = " "
WRRB = VAL("0000")
WRRB = RRB
WRDK = RDK
WROPIS = ROPIS
WRBR = RBR
WRDATDOK = RDATDOK
WRPV = RPV
WRPN = RPN
WRPN1 = RPN1
WRPBEZ = RPBEZ
WRPBEZ06 = RPBEZ06
WRPBEZ2 = RPBEZ2
WRPBEZ4 = RPBEZ4
WRUSP = RUSP
WRUSBEZP = RUSBEZP
WRPLACP = RPLACP
WRSVEP = RSVEP
WRNVRRR = RNVRRR
WRVSP = RVSP
WRRUCP = RRUCP
WROBPOR = ROBPOR
WRPRODV = RPRODV
WRMIN = RMIN
WRDOP = RDOP
WRRASF = RRASF
WRRASUK = RRASUK
WRZIRU = RZIRU
WRZIRI = RZIRI
DO PAZSMIZ
IF V = 1
EXIT
ENDIF
@ 23,24 SAY "upis   D/N  ? " GET DN PICTURE "!" VALID(DN $ "DN")
READ
IF Lastkey() = 27
EXIT
ENDIF
IF DN = "N"
LOOP
ENDIF
    REPLACE RRB WITH WRRB
    REPLACE RDK WITH WRDK
    REPLACE ROPIS WITH WROPIS
    REPLACE RBR WITH WRBR
    REPLACE RDATDOK WITH WRDATDOK
    REPLACE RPV WITH WRPV
    REPLACE RPN WITH WRPN
    REPLACE RPN1 WITH WRPN1
    REPLACE RPBEZ WITH WRPBEZ
    REPLACE RPBEZ06 WITH WRPBEZ06
    REPLACE RPBEZ2 WITH WRPBEZ2
    REPLACE RPBEZ4 WITH WRPBEZ4
    REPLACE RUSP WITH WRUSP
    REPLACE RUSBEZP WITH WRUSBEZP
    REPLACE RPLACP WITH WRPLACP
    REPLACE RSVEP WITH WRSVEP
    REPLACE RNVRRR WITH WRNVRRR
    REPLACE RVSP WITH WRVSP
    REPLACE RRUCP WITH WRRUCP
    REPLACE ROBPOR WITH WROBPOR
    REPLACE RPRODV WITH WRPRODV
    REPLACE RMIN WITH WRMIN
    REPLACE RDOP WITH WRDOP
    REPLACE RRASF WITH WRRASF
    REPLACE RRASUK WITH WRRASUK
    REPLACE RZIRU WITH WRZIRU
    REPLACE RZIRI WITH WRZIRI
B = B + 1
ENDDO
*
USE
*CLEAR ALL
CLEAR SCREEN
RETURN


