* - nije pregledano - *
CLEAR SCREEN
SET DATE GERMAN
USE KPCPRI INDEX KPCPRIDK
DO WHILE .T.
GO TOP
ESCKEY()
DAT1 = DATE()
DAT2 = DATE()
UKUPNO = VAL("000000000000.00")
@ 10,10 SAY "UKUPNI PRIHOD OD DATUMA" GET DAT1
@ 10,43 SAY "DO DATUMA" GET DAT2
READ
*
IF Lastkey() = 27
CLEAR SCREEN
USE
RETURN
ENDIF
DO WHILE .T.                        
IF Eof()
EXIT
ENDIF
 IF DTOS(DK) >= DTOS(DAT1) .AND. DTOS(DK) <= DTOS(DAT2)
  UKUPNO = UKUPNO + PRIH
 ENDIF
 SKIP
ENDDO
@ 20,20 SAY "UKUPNI PRIHOD ZA DATI PERIOD JE"
*SET COLO TO /W
@ 20,57 SAY UKUPNO PICTURE "999,999,999,999.99"
*SET COLO TO
WAIT " "
CLEAR SCREEN
USE
RETURN
ENDDO
        
