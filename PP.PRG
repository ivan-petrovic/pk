*******************************************
* Glavni meni za Poslovne partnere
*******************************************
LOCAL Izbor, nPredhIzbor

nPredhIzbor = 1
DO WHILE .T.

	MainWnd("GLAVNI MENI ZA POSLOVNE PARTNERE")
	Izbor = nPredhIzbor
	@ 8,27  PROMPT " UNOS POSLOVNIH PARTNERA  " MESSAGE "OTVARANJE POSLOVNOG PARTNERA"
	@ 10,27 PROMPT " BRISANJE POS. PARTNERA   " MESSAGE "BRISANJE POSLOVNOG PARTNERA"
	@ 12,27 PROMPT " IZMENE POSLO. PARTNERA   " MESSAGE "IZMENA POSLOVNOG PARTNERA"
	@ 14,27 PROMPT " UPITI U POSL. PARTNERE   " MESSAGE "RAZNI UPITI"
	@ 16,27 PROMPT " STAMPE POSLO. PARTNERA   " MESSAGE "STAMPANJE POSLOVNIH PARTNERA"
	@ 18,27 PROMPT " STAMPA KARTICE POS.PART. " MESSAGE "STAMPA POTRAZIVANJA POSLOVNOG PARTNERA PO KALKULACIJAMA"

	CLEAR TYPEAHEAD
	MENU TO Izbor
	* nPredhIzbor = Izbor
	
	DO CASE
		CASE Lastkey() = 27
			EXIT
		CASE Izbor = 1
			DO PPUNOS
		CASE Izbor = 2
			DO PPBRIS
		CASE Izbor = 3
			DO PPIZME
		CASE Izbor = 4
			DO PPUPMENI
		CASE Izbor = 5
			DO PPSTMENI
		CASE Izbor = 6
			DO KARTDOB
	ENDCASE

ENDDO

RETURN
