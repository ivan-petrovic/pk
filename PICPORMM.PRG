*****************************************
* Glavni meni za preracunate PDV stope
*****************************************
LOCAL Izbor, nPredhIzbor

nPredhIzbor = 1
DO WHILE .T.

	MainWnd("PRERACUNATI POREZ KAFA,PICE,CIGARETE")

	Izbor = nPredhIzbor
	@ 8,27  PROMPT " IZRACUNAVANJE PRERACUNATIH STOPA " MESSAGE "UNOS STAVKI ZIRO RACUNA"
	@ 10,27 PROMPT " PREGLED,IZMENA,BRISANJE PRER.STO " MESSAGE "PREGLED,IZMENA,BRISANJE PRERACUNATIH STOPA"
	@ 12,27 PROMPT " RACUNANJE POREZA PO PRE. STOPAMA " MESSAGE "IZRACUNAVANJE,PREGLED I STAMPANJE POREZA PO PRERACUNATIM STOPAMA"

	CLEAR TYPEAHEAD
	MENU TO Izbor
	* nPredhIzbor = Izbor

	DO CASE
		CASE Lastkey() = 27
			EXIT
		CASE Izbor = 1
			DO IZKOEKPI
		CASE Izbor = 2
			DO KOELKPI
		CASE Izbor = 3
			DO KOBRKPI
	ENDCASE

ENDDO

RETURN
