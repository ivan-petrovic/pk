*******************************************
* Glavni meni za izbor posla prihoda
*******************************************
LOCAL Izbor, nPredhIzbor

nPredhIzbor = 1
DO WHILE .T.

	MainWnd("K NJ I G A   P R I H O D A")

	Izbor = nPredhIzbor
	@ 6 ,27 PROMPT " UNOS PRIHODA STR-a        " MESSAGE "UNOS PRIHODA ZA STR"
	@ 7 ,27 PROMPT " UNOS PRIHODA KOMISION     " MESSAGE "UNOS STAVKI PRIHODA ZA KOMISION"
	@ 9 ,27 PROMPT " IZMENA PRIHODA STR-a      " MESSAGE "IZMENA STAVKE PRIHODA STR-a"
	@ 10,27 PROMPT " IZMENA PRIHODA KOMISIONA  " MESSAGE "IZMENA STAVKE PRIHODA KOMISIONA"
	@ 12,27 PROMPT " LISTANJE PRIHODA STR-a    " MESSAGE "LISTANJE KNJIGE OD POCETKA DO KRAJA"
	@ 13,27 PROMPT " LISTANJE PRIHODA KOMISION " MESSAGE "LISTANJE KNJIGE OD POCETKA DO KRAJA"
	@ 15,27 PROMPT " UPITI ZBIROVA STR-a       " MESSAGE "ZBIROVI PO PERIODIMA"
	@ 16,27 PROMPT " UPITI ZBIROVA KOMISION    " MESSAGE "ZBIROVI PO PERIODIMA"
	@ 18,27 PROMPT " PUNJENJE KNJIGA PAZAROM   " MESSAGE "AKO NEMA PAZARA U KNJIGAMA OVAJ PROGRAM TO PUNI"
	@ 20,27 PROMPT " STAMPA KNJIGE PRIHODA     " MESSAGE "STAMPA PRIHODA IZ KNJIGE PRIHOD-RASHOD"

	CLEAR TYPEAHEAD
	MENU TO Izbor
	* nPredhIzbor = Izbor
	
	DO CASE
		CASE Lastkey() = 27
			EXIT
		CASE Izbor = 1
			DO PAZSTR30
		CASE Izbor = 2
			DO PAZKOM30
		CASE Izbor = 3
			DO PAZSTRIZ
		CASE Izbor = 4
			DO PAZKOMIZ
		CASE Izbor = 5
			DO GKLISTN
		CASE Izbor = 6
			DO GKLISTKN
		CASE Izbor = 7
			DO GKZBIR
		CASE Izbor = 8
			DO GKZBIRK
		CASE Izbor = 9
			DO DODPAZ
		CASE Izbor = 10
			DO GKSTAMP9
	ENDCASE

ENDDO

RETURN
