*******************************************
* Glavni meni za izbor posla povracaja robe
*******************************************
LOCAL Izbor, nPredhIzbor

nPredhIzbor = 1
DO WHILE .T.

	MainWnd("GLAVNI MENI ZA POVRACAJ ROBE")

	Izbor = nPredhIzbor

	@ 6 ,27 PROMPT " INTERNA DOSTAVNICA         " MESSAGE "IZRADA INTERNE DOSTAVNICE ROBA IZ STR_a U STR"
	@ 8 ,27 PROMPT " POVRACAJ ARTIKALA          " MESSAGE "IZRADA KALKULACIJA POVRACAJA"
	@ 10,27 PROMPT " POVRACAJ ARTIKALA NAB.CENA " MESSAGE "IZRADA KALKULACIJA POVRACAJA, UNOS NABAVNE CENE"
	@ 12,27 PROMPT " STAMPA POVRACAJA           " MESSAGE "STAMPA KALKULACIJE POVRACAJA"
	@ 14,27 PROMPT " STAMPA INTERNE DOSTAVNICE  " MESSAGE "STAMPANJE INTERNE DOSTAVNICE"
	@ 16,27 PROMPT " INTERNA DOSTAVNICA NAB.CEN." MESSAGE "IZRADA INTERNE DOSTAVNICE ROBA IZ STR_a U STR SA NAB.CEN."

	CLEAR TYPEAHEAD
	MENU TO Izbor
	* nPredhIzbor = Izbor
	
	DO CASE
   	CASE Lastkey() = 27
      	EXIT
		CASE Izbor = 1
			DO STRPREN
		CASE Izbor = 2
			DO STRPOV
		CASE Izbor = 3
			DO STRPOVN
		CASE Izbor = 4
			DO STPOVRA
		CASE Izbor = 5
			DO STRPRENS
		CASE Izbor = 6
			DO STRPREN1
	ENDCASE

ENDDO

RETURN
