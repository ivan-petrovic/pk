LOCAL wRRb

CLEAR SCREEN
@ 0, 0 TO 24,79 DOUBLE
@ 1,1 SAY Centriraj("NUMERISEM KNJIGU PRIHOD-RASHOD", 78)

SELECT 1
USE RASHODN INDEX RASHINDN
SELECT 2
USE POREZN INDEX POREZNBB,POREZNB,POREZND

SELECT 1

wRRb = 1
DO WHILE ! Eof()
   REPLACE RRB WITH wRRb

	SELECT 2
   SEEK A->RBR
   IF Found()
      REPLACE RBPK1 WITH wRRb
   ENDIF

   wRRb = wRRb + 1
	SELECT 1
   SKIP
ENDDO

CLOSE DATABASES
MessageBox("ZAVRSENA NUMERACIJA! Bilo sta za nastavak...")

RETURN
