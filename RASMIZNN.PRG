SET CONFIRM ON
SET DATE GERMAN
CLEAR SCREEN
ESCKEY()
@ 1,0 TO 24,79 DOUBLE
D = 1
DO WHILE D = 1
@ 1,15 SAY " IZMENA PODATAKA U KNJIZI PRIHODA I RASHODA "
*
@ 2,1 SAY "REDNI BROJ                   "
@ 2,31 SAY WRRB PICTURE "9999"
@ 2,40 SAY "DATUM KNJIZENJA" GET WRDK
