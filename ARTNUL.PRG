LOCAL DN

MainMask("SVE KOLICINE LAGERA NA NULU DA NE BI PITAO NA KALKULACIJI")

SELECT 0
USE ARTPANE

DN = " "
@ 22,10 SAY "Sve kolicine na nulu (d/n) ? " GET DN ;
        PICTURE "!" VALID (DN $ "DN")
READ

IF Lastkey() = 27 .OR. DN = "N"
	CLOSE ARTPANE
	RETURN
ENDIF

@ 10,10 SAY ARTSIF PICTURE "9999999999999"

DO WHILE ! Eof()
	REPLACE ARTKOL WITH 0
	SKIP
ENDDO

CLOSE ARTPANE

RETURN
