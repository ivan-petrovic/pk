********************************************
* Glavni meni za evidenciju placenog poreza
********************************************
LOCAL Izbor, nPredhIzbor

nPredhIzbor = 1
DO WHILE .T.

	MainWnd("P L A C E N    P O R E Z")

	Izbor = nPredhIzbor
	@ 6 ,27 PROMPT " UNOS PLACENOG POREZA     " MESSAGE "UNOS STAVKI PLACENOG POREZA"
	@ 8 ,27 PROMPT " IZMENA STAVKE PLACENOG   " MESSAGE "IZMENA STAVKI PLACENOG POREZA"
	@ 10,27 PROMPT " LISTANJE STAVKI PLACENOG " MESSAGE "LISTANJE STAVKI PLACENOG POREZA"
	@ 12,27 PROMPT " ZBIROVI STAVKI PLACENOG  " MESSAGE "ZBIROVI STAVKI PLACENOG POREZA PO INTERVALIMA"
	@ 14,27 PROMPT " STAMPA STAVKI PLACENOG   " MESSAGE "STAMPANJE STAVKI PLACENOG OD DATUMA DO DATUMA"
	@ 16,27 PROMPT " STAMPA STAVKI PLACENOG 1 " MESSAGE "STAMPANJE STAVKI PLACENOG ZA POSELEDNJIH 7 STAVKI"
	@ 18,27 PROMPT " KUMULATIV UPLATA         " MESSAGE "IZRACUNAVANJE KUMULATIVA UPLATA"
	@ 20,27 PROMPT " PREPIS STARE BAZE U NOVU " MESSAGE "PREPIS STARE U NOVU BAZU SA PERIODOM I PODPERIODOM"
	@ 22,27 PROMPT " PREPIS NOVE BAZE U NOVU  " MESSAGE "PREPIS U NOVE BAZU U NOVU SA PERIODOM I PODPERIODOM"

	CLEAR TYPEAHEAD
	MENU TO Izbor
   * nPredhIzbor = Izbor

	DO CASE
		CASE Lastkey() = 27
			EXIT
		CASE Izbor = 1
			DO UPLACPOR
		CASE Izbor = 2
			DO IPLACPOR
		CASE Izbor = 3
			DO LPLACPOR
		CASE Izbor = 4
			DO ZPLACPOR
		CASE Izbor = 5
			DO SPLACPOR
		CASE Izbor = 6
			DO SPLACPO1
		CASE Izbor = 7
			DO KUMULUPL
		CASE Izbor = 8
			DO PREPPORS
		CASE Izbor = 9
			DO PREPPOR
	ENDCASE

ENDDO

RETURN
